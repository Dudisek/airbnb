<div class="row">	
	<% if @listing_tags %>
		<h2><%="Result".pluralize(@listing_tags.count)%> for tag "<%=@tag_name%>"</h2>
		<% @listing_tags.each do |list| %>
	  	<div class="col-lg-2 col-sm-3 col-xs-4">
		    <a href='/listings/<%=list.id%>'>
		      <img src="<%=list.picture[0]%>" class="thumbnail img-responsive">
		    </a>
		  </div>
		<%end%>
	<%end%>
</div>